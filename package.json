{
  "name": "media-app",
  "version": "3.0.26",
  "description": "Media App",
  "main": "./out/app.js",
  "scripts": {
    "release": "tsc && grunt move && electron-builder -p \"onTagOrRelease\"",
    "build": "tsc && grunt move && electron-builder"
  },
  "build": {
    "appId": "com.sanderronde.mediaapp",
    "productName": "Media App",
    "publish": {
      "provider": "github",
      "owner": "SanderRonde"
    },
    "directories": {
      "buildResources": "buildResources"
    },
    "mac": {
      "category": "public.app-category.entertainment",
      "icon": "buildResources/icon.icns"
    },
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "category": "Video",
      "target": [
        "deb",
        "tar.gz"
      ],
      "icon": "./"
    },
    "files": [
      "out/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "asarUnpack": [
      "./**/*",
      "./package.json",
      "!./backgroundLibs/**/*",
      "./widevine/**/*",
      "./backgroundLibs/adblocking/easylist.txt",
      "./backgroundLibs/remote/client/**.*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SanderRonde/media-app.git"
  },
  "keywords": [
    "youtube",
    "music",
    "playlist",
    "netflix",
    "subscriptions",
    "app"
  ],
  "author": {
    "name": "Sander Ronde",
    "email": "awsdfgvhbjn@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SanderRonde/media-app/issues"
  },
  "homepage": "https://github.com/SanderRonde/media-app#readme",
  "dependencies": {
    "abp-filter-parser": "^0.2.0",
    "auto-launch": "^5.0.1",
    "electron-context-menu": "^0.9.1",
    "electron-updater": "^2.17.4",
    "firebase": "^4.2.0",
    "fs": "0.0.1-security",
    "jade": "^1.11.0",
    "logger": "^0.0.1",
    "md5": "^2.2.1",
    "path": "^0.12.7",
    "sw-toolbox": "^3.6.0",
    "url": "^0.11.0",
    "websocket": "^1.0.24"
  },
  "devDependencies": {
    "@types/jade": "^0.0.30",
    "@types/md5": "^2.1.32",
    "electron": "1.7.9",
    "electron-builder": "^19.49.0",
    "electron-packager": "^8.2.0",
    "electron-prebuilt": "^1.4.5",
    "github": "^11.0.0",
    "grunt": "^1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-exec": "^3.0.0",
    "semver": "^5.4.1",
    "typescript": "2.6.1"
  }
}
