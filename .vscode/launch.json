{
  "version": "0.2.0",
  "configurations": [
	  {
		  "type": "node",
		  "request": "launch",
		  "name": "Launch debugging",
		  "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron",
			"program": "${workspaceRoot}/app/app.ts",
			"args": [
				"--debugging"
			],
			"sourceMaps": true,
			"protocol": "legacy"
		}, {
		  "type": "node",
		  "request": "launch",
		  "name": "Launch without debug",
		  "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron",
			"program": "${workspaceRoot}/app/app.ts",
			"sourceMaps": true,
			"protocol": "legacy"
		}
  ]
}